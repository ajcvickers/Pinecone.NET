<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net6.0;net8.0</TargetFrameworks>
    <WarningsAsErrors>nullable</WarningsAsErrors>
    <ImplicitUsings>enable</ImplicitUsings>
    <LangVersion>preview</LangVersion>
    <Features>strict</Features>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.13.0" />
    <PackageReference Include="Grpc.Net.ClientFactory" Version="2.32.0" />
    <PackageReference Include="Grpc.Tools" Version="2.53.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <Protobuf
      Include="Grpc\VectorService.proto"
      GrpcServices="Client"
      EnableDefaultProtobufItems="true"
      AdditionalImportDirs="..\deps\api-common-protos;..\deps\grpc-gateway">
        <SourceUrl>https://raw.githubusercontent.com/pinecone-io/go-pinecone/main/pinecone_grpc/vector_service.proto</SourceUrl>
    </Protobuf>
  </ItemGroup>
</Project>